{
  "compilerOptions": {
    "target": "ESNext",
    "strictPropertyInitialization": false,
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": "./src",
    "paths": {
      "@container/*": ["./container/*"]
    }
  }
}
